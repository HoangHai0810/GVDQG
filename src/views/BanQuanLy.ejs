                                                signUpBtn.onclick = scrollPasswordName;
                                            }
                                        });

                                        var checkPasswordInput = document.getElementById("ism-password2");
                                        checkPasswordInput.addEventListener('input', () => {
                                            var passwordInput = document.getElementById("ism-password2").value;
                                            var isValidPassword1 = checkPassword1(passwordInput);
                                            var Box1 = document.getElementById('box1');
                                            if (isValidPassword1) {
