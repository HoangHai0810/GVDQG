                                                checkPasswordInput.classList.remove('error-border');
                                            }

                                            var signUpBtn = document.getElementById('signUpBtn');
                                            if (isValidPassword1 && isValidPassword2 && isValidPassword3 && isValidPassword4 && userNameCheck) {
                                                signUpBtn.type = 'submit';
                                                signUpBtn.onclick = replaceTab;
                                            } else {
                                                signUpBtn.type = 'button';
                                                if (!userNameCheck) {
                                                    signUpBtn.onclick = scrollUserName;
                                                } else {
                                                    signUpBtn.onclick = scrollPasswordName;
                                                }
                                            }
                                        });
                                        var passwordInput = document.getElementById("ism-password2").value;
