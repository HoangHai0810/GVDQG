        <script>
            var search = document.querySelector(".searchBtn");

            search.addEventListener("click", function () {
                if (document.querySelector(".searchBar").classList.contains("open")) {
                    document.querySelector(".searchBar").classList.remove("open");
                    document.querySelector()
                } else {
                    document.querySelector(".searchBar").classList.add("open");
                }
                if (search.classList.contains("active")) {
                    search.classList.remove("active");
                } else {
                    search.classList.add("active");
                }
            });

            const logo = document.querySelector('.logo-resizable');

            window.addEventListener('scroll', function () {
                var limit = 100;

                // Lấy giá trị cuộn chuột hiện tại
                var scrollValue = window.scrollY;

                // Tính toán kích thước mới của logo
                var newSize = Math.max(150 - scrollValue, limit);

                // Cập nhật kích thước của logo
                logo.style.height = newSize + 'px';

                var fixedPosition = 60;

                if (scrollValue >= fixedPosition) {
                    // Thanh navbar cố định ở đầu trang
                    document.getElementById('navbar').style.position = 'fixed';
                    document.getElementById('navbar').style.top = '0';
                } else {
                    // Thanh navbar theo dòng chảy bình thường
                    document.getElementById('navbar').style.position = 'static';
                    document.getElementById('navbar').style.top = 'auto';
                }
            });
            // Lắng nghe sự kiện khi tab được click
            const tabLinks = document.querySelectorAll('nav ul li a');

            //Lắng nghe sự kiện khi nút đăng nhập được click
            const signinLink = document.querySelector('.fantasySignIn');

            signinLink.addEventListener('click', (e) => {
                // Loại bỏ lớp active từ tất cả các tab
                tabLinks.forEach((tab) => {
                    tab.classList.remove('active');
                });
                // Ẩn tất cả các nội dung tab
                const tabContents = document.querySelectorAll('.tab-content');
                tabContents.forEach((tabContent) => {
                    tabContent.style.display = 'none';
                });
                // Hiển thị nội dung của tab đăng nhập và thêm lớp active cho tab
                const selectedTabContent = document.getElementById('dangnhap');
                selectedTabContent.style.display = 'block';
                link.classList.add('active');
            })

            tabLinks.forEach((link) => {
                link.addEventListener('click', (e) => {
                    e.preventDefault();

                    // Loại bỏ lớp active từ tất cả các tab
                    tabLinks.forEach((tab) => {
                        tab.classList.remove('active');
                    });

                    // Ẩn tất cả các nội dung tab
                    const tabContents = document.querySelectorAll('.tab-content');
                    tabContents.forEach((tabContent) => {
                        tabContent.style.display = 'none';
                    });

                    // Lấy tab được click
                    const selectedTab = e.target.getAttribute('data-tab');

                    // Hiển thị nội dung của tab được click và thêm lớp active cho tab
                    const selectedTabContent = document.getElementById(selectedTab);
                    selectedTabContent.style.display = 'block';
                    link.classList.add('active');
                });
            });
        </script>

