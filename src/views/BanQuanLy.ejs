                initializeTable();

                function handleSubmit(event) {
                    event.preventDefault();

                    // Lấy thông tin từ các trường nhập
                    const teamName = document.getElementById('id-teamName').value;
                    const coach = document.getElementById('id-coach').value;
                    const homeGround = document.getElementById('id-homeGround').value;
                    const homeJerseyColor = document.getElementById('id-homeJerseyColor').value;
                    const awayJerseyColor = document.getElementById('id-awayJerseyColor').value;
                    const playerData = hot.getData();
                    //Kiểm tra khóa chính 
                    var checkTeamName = false;
                    for (let i = 0; i < DOIBONG.length; i++) {
                        if (teamName === DOIBONG[i].tenDoiBong) {
                            checkTeamName = true;
                        }
                    }

                    var checkCTToiThieuToiDa = true;
                    var checkCTNNToiDa = true;
                    var checkNULLNgaySinh = true;
                    console.log(hot.countRows());
                    let countCT = hot.countRows();
                    let countNN = 0;


                    //Kiểm tra số lượng CTNN
                    for (let i = 0; i < playerData.length; i++) {
                        if (playerData[i][7] === 'Cầu thủ nước ngoài') {
                            countNN++;
                        }
                    }
                    //Kiểm tra số lượng cầu thủ
                    if (countCT < SoCTToiThieu || countCT > SoCTToiDa) {
                        checkCTToiThieuToiDa = false;
                    }

                    if (countNN > CTNNToiDa) {
                        checkCTNNToiDa = false;
                    }
                    //Kiểm tra ngày sinh không null
                    for (let i = 0; i < playerData.length; i++) {
                        if (playerData[i][3] === null || playerData[i][3] === 'YYYY-MM-DD') {
                            checkNULLNgaySinh = false;
                            break;
                        }
                    }
                    console.log(playerData);
                    if (!checkTeamName && checkCTToiThieuToiDa && checkCTNNToiDa && checkNULLNgaySinh) {

                        // Tạo đối tượng đại diện cho đội bóng
                        const team = {
                            teamName,
                            coach,
                            homeGround,
                            homeJerseyColor,
                            awayJerseyColor,
                            players: playerData.slice(0) // Bỏ qua hàng đầu tiên (tiêu đề)
                        };

                        // Cập nhật giá trị của hidden input
                        document.getElementById('playerData').value = JSON.stringify(team.players);

                        // Gửi form
                        document.getElementById('teamForm').submit();
                    }
                    else {
                        if (checkTeamName) {
                            errorTeamName.innerHTML = 'Đội bóng đã tồn tại';
                            errorTeamName.style.color = '#e53535';
                            teamNameBox.classList.add('error-border');
                        }
                        if (!checkCTToiThieuToiDa) {
                            var errorSoCTinner = 'Số lượng cầu thủ phải từ ' + SoCTToiThieu + ' đến ' + SoCTToiDa;
                            errorSoCT.innerHTML = errorSoCTinner;
                            errorSoCT.style.color = '#e53535';
                        }
                        else {
                            errorSoCT.innerHTML = '';
                        }
                        if (!checkCTNNToiDa) {
                            var errorCTNNinner = 'Số lượng cầu thủ nước ngoài tối đa là ' + CTNNToiDa;
                            errorCTNN.innerHTML = errorCTNNinner;
                            errorCTNN.style.color = '#e53535';
                        }
                        else {
                            errorCTNN.innerHTML = '';
                        }
                        if (!checkNULLNgaySinh) {
                            errorNgaySinh.innerHTML = 'Ngày sinh không được để trống';
                            errorNgaySinh.style.color = '#e53535';
                        }
                        else {
                            errorNgaySinh.innerHTML = '';
                        }
                    }
                }
                //Bỏ error
                const errorTeamName = document.getElementById('errorTeamName');
                const errorSoCT = document.getElementById('errorSoCT');
                const errorCTNN = document.getElementById('errorCTNN');
                const errorNgaySinh = document.getElementById('errorNgaySinh');
                const teamNameBox = document.getElementById('id-teamName');

                teamNameBox.addEventListener('keyup', () => {
                    if (event.key !== 'Enter') {
                        errorTeamName.innerHTML = '';
                        teamNameBox.ClassList.remove('error-border');
                    }
                })

                const teamForm = document.getElementById('teamForm');
                teamForm.addEventListener('submit', handleSubmit);
            </script>
