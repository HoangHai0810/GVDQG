                                if (dataCauThu[i].quocTich == 'Cameroon') { flag = 'CM' };%>
                                <span class="flag <%=flag%>" id="flags"></span>  
                                <script>
                                    var dataCauThu = '<%- dataCauThu[i].quocTich %>';
                                    flag = document.getElementById('flags');
                                    if (dataCauThu == 'Việt Nam')
                                    {
                                        flag.classList.add('VN');
                                    }
                                </script>
                                <span class="playerCountry"><%= dataCauThu[i].quocTich %></span>
                            </td> 
                            <td class="hide-s">
                                <%= dataCauThu[i].tenDoiBong %>
                            </td>
                        </tr> 
                    <% } %>
                    </table>
                </div>
