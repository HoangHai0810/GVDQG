    <script src="https://cdnjs.cloudflare.com/ajax/libs/jqueryui/1.12.1/jquery-ui.min.js"></script>
    <script>
        $(function() {
            $('.player-card').draggable({
                revert: 'invalid',
                cursor: 'move',
                zIndex: 100,
                helper: 'clone',
                start: function(event, ui) {
                    ui.helper.addClass('dragging');
                },
                stop: function(event, ui) {
                    ui.helper.removeClass('dragging');
                }
            });

            $('.position').droppable({
                accept: '.player-card',
                tolerance: 'pointer',
                drop: function(event, ui) {
                    const draggable = ui.draggable;
                    const droppable = $(this);

                    const playerClone = draggable.clone();
                    const playerName = draggable.text();
                    const positionName = droppable.data('position');
                    

                    const currentPlayer = droppable.find('.player-card');
                    currentPlayer.remove();
                    currentPlayer.removeClass('ui-draggable-handle'); // Xóa lớp ui-draggable-handle
                    currentPlayer.children().remove()
                    $('.players-section').append(currentPlayer);

                    $('.lineup-section .player-card, .players-section .player-card').draggable({
                        revert: 'invalid',
                        cursor: 'move',
                        zIndex: 100,
                        helper: 'clone',
                        start: function(event, ui) {
                            ui.helper.addClass('dragging');
                        },
                        stop: function(event, ui) {
                            ui.helper.removeClass('dragging');
                        }
                    });
                    
                
                    $('.players-section').droppable({
                        accept: '.lineup-section .player-card',
                        tolerance: 'pointer',
                        drop: function(event, ui) {
                            const draggable = ui.draggable;
                            const droppable = $(this);
                
                            const playerClone = draggable.clone();
                            const playerName = draggable.text();
                
                            playerClone.addClass('player-card');
                            droppable.append(playerClone);
                
                            playerClone.draggable({
                                revert: 'invalid',
                                cursor: 'move',
                                zIndex: 100,
                                helper: 'clone',
                                start: function(event, ui) {
                                    ui.helper.addClass('dragging');
                                },
                                stop: function(event, ui) {
                                    ui.helper.removeClass('dragging');
                                }
                            });
                
                            draggable.remove();
                        }
                    })


                    playerClone.addClass('player-card');
                    droppable.empty();
                    droppable.append(playerClone);
                    playerClone.append('<div class="player-info" style="left:35%">'  + positionName + '</div>');
                    const playerInfo = droppable.find('.player-info');
                    playerInfo.hide();

                    playerClone.draggable({
                        revert: 'invalid',
                        cursor: 'move',
                        zIndex: 100,
                        helper: 'clone',
                        start: function(event, ui) {
                            ui.helper.addClass('dragging');
                        },
                        stop: function(event, ui) {
                            ui.helper.removeClass('dragging');
                        }
                    });

                    draggable.remove();
                }
                
            });

            $(document).on('click', '.position', function() {
                const playerInfo = $(this).find('.player-info');
                $('.player-info').hide();
                if ($(this).hasClass('selected'))
                {
                    playerInfo.show();
                }
                else
                {
                    playerInfo.hide();
                }
                
            });
            
        });
            
            function toggleSelected(element) {
                if (element.classList.contains('selected')) {
                    element.classList.remove('selected');
                } else {
                    $('.position').removeClass('selected');
                    element.classList.add('selected');
                }
            }
    </script>
